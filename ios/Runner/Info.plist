	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<true/>
	</dict>
	<key>NSAppleMusicUsageDescription</key>
	<string>This app access photo library</string>
	<key>NSBluetoothAlwaysUsageDescription</key>
	<string>yes</string>
	<key>NSBluetoothPeripheralUsageDescription</key>
	<string>yes</string>
	<key>NSCameraUsageDescription</key>
	<string>yes</string>
	<key>NSDocumentDirectory</key>
	<string>yes</string>
	<key>NSDocumentsFolderUsageDescription</key>
	<string>We need access to read files from the Documents folder</string>
	<key>NSFaceIDUsageDescription</key>
	<string>Why is my app authenticating using face id?</string>
	<key>NSLibraryFolderUsageDescription</key>
	<string>We need access to read files from the Library folder</string>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>Our app needs your location for XYZ reasons.</string>
	<key>NSLocationAlwaysUsageDescription</key>
	<string>This app needs access to location when in the background.</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>Reason why we need access to your location</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>yes</string>
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>To save edited photos to your device's photo gallery, we need your permission</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>This app needs access to your photo library to let you upload photos to your profile</string>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
	<key>UIBackgroundModes</key>
	<array>
		<string>remote-notification</string>
	</array>
	<key>UIFileSharingEnabled</key>
	<true/>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UIRequiresFullScreen</key>
	<true/>
	<key>UIStatusBarStyle</key>
	<string>UIStatusBarStyleDarkContent</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
	</array>
	<key>UIViewControllerBasedStatusBarAppearance</key>
	<false/>
	<key>io.flutter.embedded_views_preview</key>
	<true/>
